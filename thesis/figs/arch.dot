digraph architecture {
        size="10,25"
        ratio=0.2
        rankdir=LR
        nodesep=0.1
        ranksep=0.5
        node [shape = box]
        "forráskód" [shape = egg, style = filled]
        "átnevezés" [shape = hexagon]
        "helyettesítés" [shape = hexagon]
        "forráskód" -> "AST"
      "AST" -> "helyettesítés" [label = "definíciók"]
      "AST" -> "átnevezés" [label = "nevek"]
      "átnevezés" -> "helyettesítés" [style=dashed, label = "név-azonosító párosítások"]
      "helyettesítés" -> "gráfátíró rendszer" [label = "helyettesített definíciók"]
      "átnevezés" -> "gráfátíró rendszer"
}